{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@/(.*)$": "<rootDir>/src/$1",
    "@prisma_module/(.*)$": "<rootDir>/src/prisma/$1",
    "@stock/(.*)$": "<rootDir>/src/stock/$1",
    "@products/(.*)$": "<rootDir>/src/products/$1",
    "@tenant/(.*)$": "<rootDir>/src/tenant/$1",
    "@auth/(.*)$": "<rootDir>/src/auth/$1"
  }
}